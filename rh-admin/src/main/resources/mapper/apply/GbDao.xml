<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ntnikka.modules.apply.dao.GbDao">

    <select id="getCids" resultType="java.util.HashMap">
        SELECT DISTINCT cid,name FROM gb_lib
    </select>

    <select id="getBids" resultType="java.util.HashMap">
        SELECT DISTINCT cid,bid,bname FROM gb_lib
    </select>

    <select id="getBids" resultType="java.util.HashMap">
        SELECT DISTINCT cid,bid,bname FROM gb_lib
    </select>

    <select id="getMids" resultType="java.util.HashMap">
        SELECT DISTINCT bid,mid,mname FROM gb_lib
    </select>

    <select id="getLids" resultType="java.util.HashMap">
        SELECT DISTINCT mid,lid,lname FROM gb_lib
    </select>

</mapper>